{
  "name": "neocad",
  "version": "0.9.0",
  "description": "The new standard for web-based CAD/BIM - Built on Fragments 3.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,html}\"",
    "type-check": "tsc --noEmit",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "docs:pull-thatopen": "node scripts/crawl-thatopen-docs.js"
  },
  "keywords": [
    "bim",
    "cad",
    "ifc",
    "fragments",
    "3d",
    "webgl",
    "architecture",
    "engineering",
    "construction"
  ],
  "author": "NEOcad Team",
  "license": "MIT",
  "dependencies": {
    "@bitbybit-dev/core": "^0.20.11",
    "@bitbybit-dev/occt": "^0.20.11",
    "@bitbybit-dev/threejs": "^0.20.11",
    "@thatopen/components": "^3.2.0",
    "@thatopen/components-front": "^3.2.0",
    "@thatopen/fragments": "^3.2.0",
    "@thatopen/ui": "^3.2.0",
    "@thatopen/ui-obc": "^3.2.0",
    "mathjs": "^15.0.0",
    "three": "^0.175.0",
    "three-bvh-csg": "^0.0.17",
    "web-ifc": "^0.0.72"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/jsdom": "^27.0.0",
    "@types/three": "^0.175.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.3.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.7"
  }
}
